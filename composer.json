{
    "name": "open-southeners/phpunit-todo-markdown-printer",
    "description": "Print your failed tests as a to do list in Markdown format",
    "license": "MIT",
    "keywords": [
        "open-southeners",
        "phpunit",
        "phpunit-logger",
        "phpunit-extension",
        "productivity",
        "markdown",
        "tests"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "me@d8vjork.com",
            "homepage": "https://d8vjork.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\PHPUnitTodoMarkdownPrinter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\PHPUnitTodoMarkdownPrinter\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}